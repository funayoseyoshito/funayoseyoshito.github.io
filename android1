<body>
  <script type="text/javascript">
    window.receiveFcmToken = function(token) {
      document.body.insertAdjacentHTML('afterend','<p>' + token +'</p>');
    }
  </script>
  <p onclick="Android.postMessage('test')">getFcmToken</p>
  <p onclick="Android.updateBadge('20')">updateBadge 20</p>
  <p onclick="Android.updateBadge('1')">updateBadge 1</p>
</body>
